file "fru_common_alias.db"
{
    pattern { id        , fruid    }
            { $(suffix) , $(fruid) }
}

file "sensor_ai_alias.db"
{
    pattern { attr       , sensinst , type , fruid       , prefix }
            { DTemp      , 1        , 1    , $(fruid)    , $(suffix)  }
            { DMaxTemp   , 2        , 1    , $(fruid)    , $(suffix)  }
            { MTemp      , 3        , 1    , $(fruid)    , $(suffix)  }
            { MMaxTemp   , 4        , 1    , $(fruid)    , $(suffix)  }
            { Volt3V3    , 1        , 2    , $(fruid)    , $(suffix)  }
            { Volt12V    , 2        , 2    , $(fruid)    , $(suffix)  }
}
